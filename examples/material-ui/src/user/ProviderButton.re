let style = ReactDOMRe.Style.make;
let addUnsafe = ReactDOMRe.Style.unsafeAddProp;
let combine = ReactDOMRe.Style.combine;
[%mui.withStyles
  "Styles"(_ =>
    {
      providerButton:
        style(
          ~display="block",
          ~color="#fff",
          ~position="relative",
          ~border="2px solid rgb(14, 30, 37)",
          ~textTransform="none",
          (),
        )
        ->addUnsafe(
            "&::before",
            style(
              ~content="''",
              ~position="absolute",
              ~display="inline-block",
              ~verticalAlign="middle",
              ~width="32px",
              ~height="40px",
              ~backgroundRepeat="no-repeat",
              ~backgroundPosition="left center",
              ~top="-2px",
              ~left="14px",
              (),
            )
            ->Utils.styleToString,
          ),
      providerGoogle:
        combine(
          style(~backgroundColor="#4285f4", ~borderColor="#366dc7", ())
          ->addUnsafe(
              "&:hover, &:focus",
              style(~backgroundColor="#366dc7", ())->Utils.styleToString,
            ),
          style()
          ->addUnsafe(
              "&::before",
              style(
                ~backgroundImage=
                  "url(data:image/svg+xml;base64,)",
                (),
              )
              ->Utils.styleToString,
            ),
        ),
      providerGitHub:
        combine(
          style(~backgroundColor="#333", ~borderColor="#000", ())
          ->addUnsafe(
              "&:hover, &:focus",
              style(~backgroundColor="#000", ())->Utils.styleToString,
            ),
          style()
          ->addUnsafe(
              "&:before",
              style(
                ~backgroundImage=
                  "url(data:image/svg+xml;base64,)",
                (),
              )
              ->Utils.styleToString,
            ),
        ),
      providerGitLab:
        combine(
          style(~backgroundColor="#e24329", ~borderColor="#b03320", ())
          ->addUnsafe(
              "&:hover, &:focus",
              style(~backgroundColor="#b03320", ())->Utils.styleToString,
            ),
          style()
          ->addUnsafe(
              "&::before",
              style(
                ~backgroundImage=
                  "url(data:image/svg+xml;base64,)",
                (),
              )
              ->Utils.styleToString,
            ),
        ),

      providerBitbucket:
        combine(
          style(~backgroundColor="#205081", ~borderColor="#14314f", ())
          ->addUnsafe(
              "&:hover, &:focus",
              style(~backgroundColor="#14314f", ())->Utils.styleToString,
            ),
          style()
          ->addUnsafe(
              "&::before",
              style(
                ~backgroundImage=
                  "url(data:image/svg+xml;base64,)",
                (),
              )
              ->Utils.styleToString,
            ),
        ),
    }
  )
];

let getProviderClass = (provider: ReactNetlifyIdentity.provider, classes) =>
  switch (provider) {
  | Google => classes.providerGoogle
  | GitHub => classes.providerGitHub
  | GitLab => classes.providerGitLab
  | Bitbucket => classes.providerBitbucket
  };

[@react.component]
let make = (~provider, ~className="") => {
  let classes = Styles.useStyles();
  let identity = ReactNetlifyIdentity.useIdentityContextSimple();
  let handleClick = _ => identity.loginProvider(provider);

  <MaterialUi_Button
    className={getProviderClass(provider, classes)}
    onClick=handleClick
    fullWidth=true
    classes=[Root(classes.providerButton ++ " " ++ className)]>
    {React.string(
       "Continue with " ++ ReactNetlifyIdentity.providerToString(provider),
     )}
  </MaterialUi_Button>;
};
